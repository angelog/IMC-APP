{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "jsx-a11y", "import"
    ],
    "rules": {
		"react/jsx-filename-extension": [
			"error",
			{
				"extension": [".jsx", ".js"]
			}
		],
		"global-require":"off",
		"import/prefer-default-export": "off",
		"no-unused-expressions": ["error", {"allowTaggedTemplates": true}]
    }
}
